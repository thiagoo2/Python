print("=============")
print("Costo de articulo")
print("=============")

print("ingrese el costo del artÃ­culo")
costo=float(input())

print("Ingrese la marca")
m=input

if costo>=2000 and m=="NOSY":
    pa=costo*0.90
    pt=pa*0.95
    
elif costo>=2000 and m!="NOSY":
    pt=costo*0.90
    
elif costo<2000 y m=="NOSY":
    pa=costo*0.95
    pt=pa+pa*0.20
    
elif costo<2000 y m!= "NOSY":
    pa=costo*1.20

print("SALIDA")
print("======")
print("Usted pagara:", pt,"soles")    
